def amicablepairs(n):
  factorsn = []         #creating an empty list to store all the factors of n
  for i in range(1, n//2+1):
    if n % i == 0:
      factorsn.append(i)

  m = sum(factorsn)       #d(a) = b
#   print(f'm = {m}')

  factorsm = []         #creating an empty list to store all the factors of m
  for j in range(1, m//2+1):
    if m % j == 0:
      factorsm.append(j)

  if sum(factorsm) == n and n!=m:   #checking if d(a) = b and d(b) = a, where a â‰  b
    return True
  else:
    return False

amicable = []
for k in range(1, 10001):
  if amicablepairs(k):
    amicable.append(k)

sumofamicablepairs = sum(amicable)    
print(sumofamicablepairs)
